{
  "name": "icl-runtime",
  "version": "0.1.3",
  "description": "JavaScript/TypeScript bindings for ICL (Intent Contract Language) via WebAssembly â€” works in Node.js, browsers, and bundlers out of the box",
  "main": "./dist/nodejs/icl_runtime.js",
  "module": "./dist/bundler/icl_runtime.js",
  "types": "./dist/bundler/icl_runtime.d.ts",
  "exports": {
    ".": {
      "types": "./dist/bundler/icl_runtime.d.ts",
      "node": {
        "import": {
          "types": "./dist/nodejs/icl_runtime.d.ts",
          "default": "./dist/nodejs/icl_runtime.mjs"
        },
        "require": {
          "types": "./dist/nodejs/icl_runtime.d.ts",
          "default": "./dist/nodejs/icl_runtime.js"
        }
      },
      "import": "./dist/bundler/icl_runtime.js",
      "default": "./dist/bundler/icl_runtime.js"
    },
    "./web": {
      "types": "./dist/web/icl_runtime.d.ts",
      "import": "./dist/web/icl_runtime.js",
      "default": "./dist/web/icl_runtime.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "keywords": [
    "icl",
    "intent",
    "contract",
    "deterministic",
    "ai",
    "wasm",
    "webassembly",
    "parser",
    "verifier"
  ],
  "author": "ICL Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ICL-System/ICL-Runtime"
  },
  "homepage": "https://github.com/ICL-System/ICL-Runtime/tree/main/bindings/javascript",
  "scripts": {
    "build": "node build.mjs",
    "test": "node tests/test_icl.mjs"
  },
  "devDependencies": {}
}
